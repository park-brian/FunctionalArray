{
    "name": "park-brian/functional-array",
    "type": "library",
    "description" : "A functional wrapper for arrays",
    "license": "MIT",
    "authors": [
        {
            "name" : "Brian Park",
            "email" : "re.public@outlook.com",
            "homepage" : "https://github.com/park-brian/",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "php": ">=7.1.0",
        "phpunit/phpunit": "^6.3",
        "codeclimate/php-test-reporter": "^0.4.4"
    },
    "autoload": {
        "psr-4": {
            "FunctionalArray\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FunctionalArrayTests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "php-cs-fixer fix -v --diff --dry-run --allow-risky=yes;"
    }
}
